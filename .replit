
hidden = ["bin", "obj"]
modules = ["dotnet-7.0"]
run = "dotnet run"

[env]
DOTNET_NOLOGO = "1"
DOTNET_CLI_TELEMETRY_OPTOUT = "1"
DOTNET_CLI_HOME = "$XDG_CACHE_HOME"

[gitHubImport]
requiredFiles = [".replit", "replit.nix"]

[nix]
channel = "stable-24_05"

[deployment]
run = ["sh", "-c", "dotnet run"]
deploymentTarget = "cloudrun"
